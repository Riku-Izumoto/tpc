- var url          = 'https://';
- var local_url    = 'http://192.168.11.3:3000/';
- var site_name    = 'Team Power Creative株式会社';
- var og_locale    = 'jp_JP';
- var og_type      = 'website';
- var twitter_card = 'summary_large_image';
- var twitter_site = '@';
- var title        = 'Team Power Creative株式会社';
- var description  = '';
- var og_img       = url + 'ogp.png';
- var favicon      = 'img/favicons/favicon.ico';

doctype html
html(lang="ja")
  head
    include ./include/_head

  body
    include ./include/_header
    main.p-thanks
      .p-thanks__first
        .p-thanks__first__bg
          img(src="img/policyTop.jpg", alt="").pc-only
          img(src="img/policyTop-sp.jpg", alt="").sp-only
          .p-thanks__first__bg__title
            span
              img(src="img/policy.svg", alt="").pc-only
              img(src="img/policy-sp.svg", alt="").sp-only
            h2.section-title プライバシーポリシー

    include ./include/_footer
    include ./include/_script
